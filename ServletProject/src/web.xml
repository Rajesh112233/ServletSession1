<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServletProject</display-name>
  
   <welcome-file-list>
   <welcome-file>home.html</welcome-file>  
  </welcome-file-list>
  <servlet>
  <servlet-name>att</servlet-name>
  <servlet-class>com.attr.S1</servlet-class>  
    </servlet> 
    <servlet>
    <servlet-name>abc</servlet-name>
    <servlet-class>com.attr.S2</servlet-class>
    </servlet>
    <servlet>
   <servlet-name>Login</servlet-name>  
    <servlet-class>com.lava.RegisterServlet</servlet-class>  
    </servlet>  
  <servlet>  
    <servlet-name>WelcomeServlet</servlet-name>  
    <servlet-class>com.lava.WelcomeServlet</servlet-class>  
    </servlet> 
     
  <servlet-mapping>  
    <servlet-name>Login</servlet-name>  
    <url-pattern>/servlet1</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping>  
    <servlet-name>WelcomeServlet</servlet-name>  
    <url-pattern>/servlet2</url-pattern>  
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>att</servlet-name>
  <url-pattern>/s1</url-pattern>
  </servlet-mapping>
   
  <servlet-mapping>
  <servlet-name>abc</servlet-name>
  <url-pattern>/s2</url-pattern>
  </servlet-mapping>  
  
 </web-app>